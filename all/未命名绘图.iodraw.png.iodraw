        <mxCell id="AFg-aJ2et3ESMCKUlOI7-106" value="&lt;font color=&quot;#7ea6e0&quot;&gt;如何知道路由器的mac地址呢?&lt;/font&gt;&lt;div&gt;&lt;font color=&quot;#7ea6e0&quot;&gt;ARP协议&lt;/font&gt;&lt;/div&gt;" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;" vertex="1" parent="1">
